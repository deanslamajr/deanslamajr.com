{
  "name": "deanslamajr.com",
  "version": "2.0.3-0",
  "description": "an about page",
  "main": "app.js",
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node public/server.js",
    "build:image": "npm version prerelease --preid=g\"$(git rev-parse --short=5 HEAD)\" && sudo docker build -t deanslamajr/dslama.net:v\"$(git describe)\" .",
    "build:front:dev": "webpack --config webpack/client.babel.js --env.dev --progress",
    "build:front:prod": "webpack --config webpack/client.babel.js --env.prod --progress -p",
    "build:back": "webpack --config webpack/server.babel.js --progress",
    "prebuild": "rimraf public",
    "build": "npm run build:back && npm run build:front:dev",
    "prebuild:prod": "rimraf public",
    "build:prod": "npm run build:back && npm run build:front:prod",
    "test": "standard --fix && jest",
    "test:update": "jest --updateSnapshot",
    "upload": "babel-node internal/upload.js",
    "update:index": "babel-node internal/find-and-replace-gzip.js",
    "deploy:minor": "npm version minor && npm run build:prod && npm run update:index && npm run upload && eb deploy -l `git describe`",
    "deploy:patch": "npm version patch && npm run build:prod && npm run update:index && npm run upload && eb deploy -l `git describe`",
    "deploy:heroku": "npm run build:prod && npm run update:index && npm run upload"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deanslamajr/deanslamajr.com.git"
  },
  "author": "Dean Slama Jr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deanslamajr/deanslamajr.com/issues"
  },
  "homepage": "https://github.com/deanslamajr/deanslamajr.com#readme",
  "dependencies": {
    "assets-webpack-plugin": "3.5.1",
    "aws-dynamodb": "0.1.64",
    "axios": "0.15.2",
    "babel-polyfill": "6.16.0",
    "bcrypt": "1.0.1",
    "body-parser": "1.15.2",
    "classnames": "2.2.5",
    "cookie-parser": "1.4.3",
    "ejs": "2.5.2",
    "express": "4.14.0",
    "helmet": "3.3.0",
    "jsonwebtoken": "7.1.9",
    "lynx": "0.2.0",
    "nconf": "0.8.4",
    "object.values": "1.0.4",
    "raven": "2.1.0",
    "react": "16.5.2",
    "react-css-modules": "3.7.10",
    "react-dom": "16.5.2",
    "react-helmet": "5.0.3",
    "react-redux": "5.0.6",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "3.6.0",
    "redux-promise-middleware": "4.2.0",
    "replace-in-file": "2.5.3",
    "source-map-support": "0.4.14",
    "styled-components": "4.0.2",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "aws-sdk": "2.77.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-styled-components": "1.8.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-es2017": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "compression-webpack-plugin": "0.4.0",
    "css-loader": "0.25.0",
    "enzyme": "2.6.0",
    "extract-text-webpack-plugin": "2.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "2.24.1",
    "identity-obj-proxy": "3.0.0",
    "jest-cli": "19.0.2",
    "postcss-cssnext": "2.8.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.1.1",
    "react-addons-test-utils": "15.4.1",
    "react-hot-loader": "4.3.11",
    "react-test-renderer": "15.5.3",
    "rimraf": "2.5.4",
    "s3-upload-stream": "1.0.7",
    "source-map-support": "0.4.14",
    "standard": "10.0.1",
    "style-loader": "0.13.1",
    "webpack": "2.4.1",
    "webpack-config-utils": "2.3.0",
    "webpack-node-externals": "1.5.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
