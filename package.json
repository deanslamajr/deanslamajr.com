    "build:front": "webpack --config config/webpack/client.dev.babel.js --progress --colors --display-error-details",
    "build:prod:front": "webpack --config config/webpack/client.prod.babel.js --progress --colors --display-error-details",
    "prebuild": "rimraf public",
