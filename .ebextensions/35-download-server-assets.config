container_commands:
  05_test_version_targetting:
    command: "sudo curl -o test.js https://s3-us-west-2.amazonaws.com/assets.dslama.net/`python -c \"import json; json_data = open('package.json'); d = json.load(json_data); print(d['version'])\"`-server.js"
  07_move_version_test:
    command: "sudo mv test.js /tmp/deployment/application/public/test.js"
