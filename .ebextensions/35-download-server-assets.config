container_commands:
  01_get_serverjs:
    command: "sudo curl -o /var/app/current/public/server.js https://s3-us-west-2.amazonaws.com/assets.dslama.net/server.js"
  02_get_serverjsmap:
    command: "sudo curl -o /var/app/current/public/server.js.map https://s3-us-west-2.amazonaws.com/assets.dslama.net/server.js.map"
  03_get_indexejs:
    command: "sudo curl -o /var/app/current/public/index.ejs https://s3-us-west-2.amazonaws.com/assets.dslama.net/index.ejs"