container_commands:
  01_get_serverjs:
    command: "sudo curl https://s3-us-west-2.amazonaws.com/assets.dslama.net/server.js > /var/app/current/public/server.js"
  02_get_serverjsmap:
    command: "sudo curl https://s3-us-west-2.amazonaws.com/assets.dslama.net/server.js.map > /var/app/current/public/server.js.map"  
  03_get_indexejs:
    command: "sudo curl https://s3-us-west-2.amazonaws.com/assets.dslama.net/index.ejs > /var/app/current/public/index.ejs"
