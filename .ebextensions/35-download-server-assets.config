container_commands:
  01_get_serverjs:
    command: "sudo curl -o server.js https://s3-us-west-2.amazonaws.com/assets.dslama.net/server.js"
  02_move_serverjs:
    command: "sudo mv server.js /tmp/deployment/application/public/server.js"
  03_get_serverjsmap:
    command: "sudo curl -o server.js.map https://s3-us-west-2.amazonaws.com/assets.dslama.net/server.js.map"
  04_move_serverjsmap:
    command: "sudo mv server.js.map /tmp/deployment/application/public/server.js.map"
  05_get_indexejs:
    command: "sudo curl -o index.ejs https://s3-us-west-2.amazonaws.com/assets.dslama.net/index.ejs"
  06_move_indexejs:
    command: "sudo mv index.ejs /tmp/deployment/application/public/index.ejs"