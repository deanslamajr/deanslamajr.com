type Link {
    name: String
    url: String
}

type HomePayload {
    links: [Link]
    version: Int
    pictureURL: String
    bio: String
    title: String
}

type Post {
    url: String
    originalPublishDate: String
    title: String
    snippet: String
}

type PostsPayload {
    posts: [Post]
    summary: String
}

type Query {
    homeQuery: HomePayload!
    postsQuery: PostsPayload!
}